generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model user {
    id String @unique @default(uuid())
    email String @unique
    password String
    first_name String?
    last_name String?
    is_admin Boolean
}

model account_balance {
    id String @unique @default(uuid())
    account_id String @unique
    current Int
}